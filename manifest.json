{
	"manifest_version": 2,
	"name": "FogBugz Time Tracking",
	"description": "Shows total time worked for current period, breakdown by case, and download option for previous period.",
	"version": "1.0",
	"icons": {
	"16": "icon.png",
	"48": "icon_48.png",
	"128": "icon_128.png"
},
"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
},
"background": {
	"page": "background.html"
},
	"permissions": [
	"downloads",
	"storage",
	"cookies",
	"alarms",
	"https://ajax.googleapis.com/",
	"http://altsource.fogbugz.com/",
	"https://altsource.fogbugz.com/"
	]
}